[user]
	name = Ram√≥n Medrano Llamas
	email = ramon.medrano.llamas@icloud.com
	signingkey = 6569E6E0
[core]
	editor = emacsclient -nw
	whitespace = trailing-space,space-before-tab
	excludesfile = ~/.gitignore_global
	autocrlf = input
	fileMode = false
[color]
	ui = auto
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[diff]
	renames = copies
	mnemonicprefix = true
[push]
	default = tracking
	signed = true
	atomic = true
[rerere]
	enabled = true
[merge]
	stat = true
[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
	lg = log --graph --decorate --oneline -100
	unstage = reset HEAD --
	last = log -1 HEAD
	visual = !gitk
	ls-ignored = ls-files --exclude-standard --ignored --others
[credential]
	helper = osxkeychain
[advice]
	statusHints = false
[branch]
	autosetupmerge = true
[pull]
	rebase = true
[status]
	submodulesummary = true
[help]
	autocorrect = 0
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[commit]
	gpgsign = true
[credential "https://source.developers.google.com"]
	helper = gcloud.sh
[init]
	defaultBranch = main
